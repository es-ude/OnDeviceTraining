    "import torch\n",
    "\n",
    "model_output = torch.tensor([1.0, 2.0, -3.0], dtype=torch.float32, requires_grad=True)\n",
    "label = torch.tensor([-5.0, -4.0, 2.0], dtype=torch.float32)\n",
    "\n",
    "loss = torch.mean((model_output - label) ** 2)\n",
    "\n",
    "loss.backward()\n",
    "\n",
    "print(model_output.grad)"
